{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"background": true,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"link": true,
			"palette": [
				{
					"name": "Black",
					"slug": "black",
					"color": "#000000"
				},
				{
					"name": "Dark Gray",
					"slug": "dark-gray",
					"color": "#292524"
				},
				{
					"name": "Medium Gray",
					"slug": "gray",
					"color": "#737373"
				},
				{
					"name": "LightGray",
					"slug": "light-gray",
					"color": "#f5f5f5"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#ffffff"
				},
				{
					"slug": "primary",
					"color": "#3b82f6",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#ef4444",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#F6F6F6",
					"name": "Tertiary"
				}
			],
			"text": true
		},
		"custom": {},
		"spacing": {
			"blockGap": true,
			"margin": false,
			"padding": false,
			"units": ["px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fontFamilies": [],
			"fontSizes": [
				{
					"name": "Extra Small",
					"size": "clamp(0.625rem, calc(0.75rem + (.875 - 0.625) * ((100vw - 20rem) / (60 - 20))), .875rem)",
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": "clamp(0.75rem, calc(0.75rem + (1 - 0.75) * ((100vw - 20rem) / (60 - 20))), 1rem)",
					"slug": "sm"
				},
				{
					"name": "Base",
					"size": "clamp(1rem, calc(1rem + (1.25 - 1) * ((100vw - 20rem) / (60 - 20))), 1.25rem)",
					"slug": "base"
				},
				{
					"name": "Medium",
					"size": "clamp(1.125rem, calc(1.25rem + (1.5 - 1.125) * ((100vw - 20rem) / (60 - 20))), 1.5rem)",
					"slug": "md"
				},
				{
					"name": "Large",
					"size": "clamp(1.75rem, calc(1.5rem + (2.25 - 1.75) * ((100vw - 20rem) / (60 - 20))), 2.25rem)",
					"slug": "lg"
				},
				{
					"name": "XL",
					"size": "clamp(2.25rem, calc(2.25rem + (3.25 - 2.25) * ((100vw - 20rem) / (60 - 20))), 3.25rem)",
					"slug": "xl"
				},
				{
					"name": "XXL",
					"size": "clamp(2.5rem, calc(3rem + (4.5 - 2.5) * ((100vw - 20rem) / (60 - 20))), 4.5rem)",
					"slug": "xxl"
				}
			],
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textDecoration": true,
			"textTransform": false
		},
		"layout": {
			"contentSize": "782px",
			"wideSize": "960px"
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontSizes": []
				}
			},
			"core/button": {
				"border": {
					"radius": false
				},
				"typography": {
					"fontSizes": []
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--dark-gray)",
					"text": "var(--wp--preset--color--light-gray)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"elements": {
			"link": {
				"border": {},
				"color": {},
				"spacing": {},
				"typography": {}
			}
		}
	},
	"customTemplates": [],
	"templateParts": []
}
